        private void bt_createFolder_Click(object sender, EventArgs e)
        {
            try
            {
                directoryPath_doc = path_WorkSpace + @"\" + TaskID + @"_" + ModuleNames + doc;
                directoryPath_rep = path_WorkSpace + @"\" + TaskID + @"_" + ModuleNames + rep;
                directoryPath_scr = path_WorkSpace + @"\" + TaskID + @"_" + ModuleNames + scr;



                if (!System.IO.Directory.Exists(directoryPath_doc) && !System.IO.Directory.Exists(directoryPath_rep) && !System.IO.Directory.Exists(directoryPath_scr))
                {
                    //System.IO.File. ();
                    // string sourceFile = System.IO.Path.Combine(directoryPath_scr, fileName);
                    System.IO.Directory.CreateDirectory(directoryPath_doc);
                    System.IO.Directory.CreateDirectory(directoryPath_rep);
                    System.IO.Directory.CreateDirectory(directoryPath_scr);

                    // print mesage
                    mesage = "Create folder successfully" + Environment.NewLine;
                    mesage += @"Task ID: " + TaskID;
                    MessageBox.Show(mesage, "CREATE FOLDER");
                }

                else
                {
                    //string sourceFile = System.IO.Path.Combine(directoryPath_scr, fileName);
                    string mesage = "Folder is already exist" + Environment.NewLine;
                    MessageBox.Show(mesage, "ERROR");
                }


            }
            catch (Exception ex)
            {
                MessageBox.Show(ex.InnerException.ToString());

            }
        }
              

