        private void bt_GetJR_Click(object sender, EventArgs e)
        {
            MyApp = new Microsoft.Office.Interop.Excel.Application();
            MyApp.Visible = false;
            MyBook = MyApp.Workbooks.Open(Get_latestFlie(@"\\bosch.com\dfsRB\DfsVN\LOC\Hc\RBVH\20_ESS\04_Projects\10_CC_DA\EI-300010\VW_PL\Project\AutoToolGen5\Estimation"));
            //sheet 2
            MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[2]; //open sheet 1
            Microsoft.Office.Interop.Excel.Range oRng = GetSpecifiedRange(TaskID, MySheet);
            if (oRng != null)
            {
             
                Microsoft.Office.Interop.Excel.Range xlRange = MySheet.UsedRange;
                File_hash = (string)(xlRange.Cells[oRng.Row, oRng.Column - 7] as Microsoft.Office.Interop.Excel.Range).Value2;
                tb_FHash.Text  = File_hash;
                Eloc = (double)(xlRange.Cells[oRng.Row, oRng.Column - 6] as Microsoft.Office.Interop.Excel.Range).Value2;
                //tb_Eloc = Eloc;
                Number_fuction = (double)(xlRange.Cells[oRng.Row, oRng.Column - 4] as Microsoft.Office.Interop.Excel.Range).Value2;
                //tb_NumFunc.Text = Number_fuction;
                string a = Convert.ToString(Eloc);
                tb_Eloc.Text = a;
                string b = Convert.ToString(Number_fuction);
                tb_NumFunc.Text = b;

            }
            else
            {
                MessageBox.Show("Text is not found");
            }


            MyBook.Close(false, Type.Missing, Type.Missing);

         
            MyApp.Quit();
        }

        private void tb_FHash_TextChanged(object sender, EventArgs e)
        {

        }

        private void tb_Eloc_TextChanged(object sender, EventArgs e)
        {

        }
       
    }
}
