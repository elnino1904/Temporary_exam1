/*******************/
using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace Ex_1_12_7
{
    public partial class Menu : Form
    {
        public Menu()
        {
            InitializeComponent();
        }
        public string path_Wsp;
        private const string path_txt_1 = @"D:\Nguyen Trung Dung\Gen5\Temp_folder\Data\save.txt";
        private void tb_pathWorkspace_TextChanged(object sender, EventArgs e)
        {
            path_Wsp = tb_pathWorkspace.Text;                  
            
        }

        private void bt_Save_Click(object sender, EventArgs e)
        {
           
            string t_addLine = path_Wsp;
            t_addLine += Environment.NewLine;
            System.IO.File.WriteAllText(path_txt_1, t_addLine);
            t_addLine = "";
            Tool_gen5.path_WorkSpace = path_Wsp;
            MessageBox.Show("Save successfully", "Config");
           
        }

        private void bt_Cancel_Click(object sender, EventArgs e)
        {
            Close();
        }

        private void Menu_Load(object sender, EventArgs e)
        {
            tb_pathWorkspace.Text = Tool_gen5.path_WorkSpace;
         
        }
    }
}

