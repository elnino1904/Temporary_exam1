        private void bt_excel_Click(object sender, EventArgs e)
        {
            if (System.IO.Directory.Exists(directoryPath_doc))
            {
                MyApp = new Microsoft.Office.Interop.Excel.Application();
                MyApp.Visible = false;
                MyBook = MyApp.Workbooks.Open(Checklist_path);
                //sheet 1
                MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[1]; //open sheet 1
                MySheet.Cells[11, 6] = "Nguyen Trung Dung"; //author
                MySheet.Cells[12, 6] = reviewer; //reviewer
                MySheet.Cells[13, 6] = reviewer; //reviewer
                MySheet.Cells[14, 6] = PDC; //PDC
                MySheet.Cells[11, 3] = "=TODAY()";          // get current day
                MySheet.Cells[12, 3] = "=TODAY()";          // get current day
                MySheet.Cells[13, 3] = "=TODAY()";          // get current day
                MySheet.Cells[14, 3] = "=TODAY()";
                //sheet Review data
                MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[2]; // open sheet 2
                MySheet.Cells[2, 2] = "Project for development of DACore in SystemC"; // project's name
                MySheet.Cells[21, 2] = file_testCpp;    // test_Modulename.cpp
                file_testHpp = ModuleNames + ".hpp";   // test_Modulename.hpp
                MySheet.Cells[14, 2] = file_testHpp;  //test_Modulename.hpp
                MySheet.Cells[14, 3] = file_hash;  // file hash cpp
                MySheet.Cells[43, 3] = reviewer;  // reviewer
                MySheet.Cells[44, 3] = reviewer;  // reviewer
                MySheet.Cells[45, 3] = PDC;  // PDC

                //sheet TestAnalysis
                MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[3]; // open sheet 3
                MySheet.Cells[2, 4] = "N.A";
                MySheet.Cells[2, 5] = "New task";
                MySheet.Cells[3, 4] = "N.A";
                MySheet.Cells[3, 5] = "New task";
                MySheet.Cells[5, 4] = "N.A";
                MySheet.Cells[5, 5] = "No OPL";
                MySheet.Cells[6, 4] = "N.A";
                MySheet.Cells[6, 5] = "No OPL";

                //sheet TC&TS review criteria
                MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[4]; // open sheet 3
                MySheet.Cells[2, 4] = "N.A";
                MySheet.Cells[2, 5] = "RQM is not available for DA core.";
                MySheet.Cells[3, 4] = "N.A";
                MySheet.Cells[3, 5] = "RQM is not available for DA core.";

                MySheet.Cells[7, 5] = "Statement: " + Statement + "%, Decision: " + Decision + "%";
                if (check_CCE)
                {
                    MySheet.Cells[7, 4] = "Y";
                }
                else MySheet.Cells[7, 4] = "N.A";
                MySheet.Cells[10, 4] = "N.A";
                MySheet.Cells[10, 5] = "RQM is not available for DA core.";
                MySheet.Cells[13, 4] = "N.A";
                MySheet.Cells[13, 5] = "no major warning";

                //sheet Pre-Delivery Check
                MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[5]; // open sheet 4
                
                if (check_OPL)
                {
                    MySheet.Cells[2, 4] = "Y";
                    MySheet.Cells[2, 5] = "all open points are closed";
                }
                else
                {
                    MySheet.Cells[2, 4] = "N.A";
                    MySheet.Cells[2, 5] = "No OPL";

                }
                MyBook.SaveAs(directoryPath_doc + @"\" + TaskID + "_" + ModuleNames + "_Review_Checklist.xlsx", Microsoft.Office.Interop.Excel.XlFileFormat.xlWorkbookDefault, Type.Missing, Type.Missing, false, Type.Missing, Microsoft.Office.Interop.Excel.XlSaveAsAccessMode.xlExclusive, Type.Missing, Type.Missing, Type.Missing, Type.Missing, Type.Missing);
                MyBook.Close();
                //Test Analysis
                MyBook = MyApp.Workbooks.Open(Test_Analysis);
                MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[1];
                MySheet.Cells[17, 3] = "Nguyen Trung Dung"; // task author
                MySheet.Cells[17, 4] = "=today()"; // get times
                MySheet = (Microsoft.Office.Interop.Excel.Worksheet)MyBook.Sheets[2];
                MySheet.Cells[2, 3] = "N.A";
                MySheet.Cells[7, 4] = "' -Doxygen";
                MySheet.Cells[7, 5] = "' -Create 1 new TC";
                
                //MessageBox.Show("Text found, position is Row:" + oRng.Row + " and column:" + oRng.Column); /////
                MyBook.SaveAs(directoryPath_doc + @"\" + TaskID + "_" + ModuleNames + "_test_analysis.xlsx", Microsoft.Office.Interop.Excel.XlFileFormat.xlWorkbookDefault, Type.Missing, Type.Missing, false, Type.Missing, Microsoft.Office.Interop.Excel.XlSaveAsAccessMode.xlExclusive, Type.Missing, Type.Missing, Type.Missing, Type.Missing, Type.Missing);
                MyBook.Close();
                MessageBox.Show("Successful", "Fill checklist");
                
            }
            else 
            {
                MessageBox.Show("No source found", "Error");
            }
        }

