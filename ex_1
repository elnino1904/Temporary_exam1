        private void Tool_gen5_Load(object sender, EventArgs e)
        {
            //add source from file
            System.IO.File.AppendAllText(path_txt, "");
            System.IO.StreamReader file = new System.IO.StreamReader(path_txt);
            string line;
            while ((line = file.ReadLine()) != null)
            {
                combSrc.Items.Add(line);

            }
            file.Close();
            //add path workspace
            System.IO.File.AppendAllText(path_txt_1, "");
            System.IO.StreamReader file1 = new System.IO.StreamReader(path_txt_1);
            string line1;
            while ((line1 = file1.ReadLine()) != null)
            {
                path_WorkSpace = line1;

            }
            file1.Close();
           //
            System.IO.File.AppendAllText(path_TaskID, "");
            System.IO.StreamReader file2 = new System.IO.StreamReader(path_TaskID);
            string line2;
            while ((line2 = file2.ReadLine()) != null)
            {
                cb_TaskId.Items.Add(line2);

            }
            file2.Close();
             //
            System.IO.File.AppendAllText(path_ModuleName, "");
            System.IO.StreamReader file3 = new System.IO.StreamReader(path_ModuleName);
            string line3;
            while ((line3 = file3.ReadLine()) != null)
            {
                cb_ModuleName.Items.Add(line3);

            }
            file3.Close();
            //
            System.IO.File.AppendAllText(path_reviewer, "");
            System.IO.StreamReader file4 = new System.IO.StreamReader(path_reviewer);
            string line4;
            while ((line4 = file4.ReadLine()) != null)
            {
                cb_Reviewer.Items.Add(line4);

            }
            file4.Close();  
            //
            System.IO.File.AppendAllText(path_PDC, "");
            System.IO.StreamReader file5 = new System.IO.StreamReader(path_PDC);
            string line5;
            while ((line5 = file5.ReadLine()) != null)
            {
                cb_PDC.Items.Add(line5);

            }
            file5.Close();  
                   
        }

