        private void btCoppy_Click(object sender, EventArgs e)
        {

            string folCantata = "Cantata";
            string folConfig = "configuration";
            string foltest = "tests";
            source_Test = sourceSandbox + @"\_test" + @"\" + ModuleNames;
            if (System.IO.Directory.Exists(source_Test))
            {
                //create test cantata folder
                string t_srcTestFolder = directoryPath_scr + @"\" + ModuleNames; // ..//Scripts/Modulde_Name
                CreateFolder(t_srcTestFolder); // create module folder
                coppyAllFile(source_Test, t_srcTestFolder); // all file after Module folder
                coppySpecFolder(folCantata, source_Test, t_srcTestFolder); //copy Cantata folder name
                string temp_srcCantata = source_Test + @"\Cantata";
                string temp_destCantata = t_srcTestFolder + @"\Cantata";
                //copy report
                string temp_srcResult = temp_srcCantata + @"\results";
                coppySpecFile(fileName_rep, temp_srcResult, directoryPath_rep);
                //string temp_destCantata = t_srcTestFolder + @"\Cantata";
                coppySpecFolder(folConfig, temp_srcCantata, temp_destCantata); //copy configuration folder name
                coppySpecFolder(foltest, temp_srcCantata, temp_destCantata); //copy tests folder name
                string temp_srcConfig = temp_srcCantata + @"\" + folConfig;
                string temp_destConfig = temp_destCantata + @"\" + folConfig;
                coppyAllFile(temp_srcConfig, temp_destConfig); // all file in folder configuration
                string temp_srcTests = temp_srcCantata + @"\" + foltest;
                string temp_destTests = temp_destCantata + @"\" + foltest;
                //coppySpecFile(fileName_testmk, temp_srcTests, temp_destTests); // copy test.mk 
                //coppySpecFile(fileName_ipg, temp_srcTests, temp_destTests); // copy ipg.cop
                coppySpecFile(fileMakefile, temp_srcTests, temp_destTests);  // copy Makefile
                string temp_srcTestsModName = temp_srcTests + @"\" + folder_testCpp;
                string temp_destTestsModName = temp_destTests + @"\" + folder_testCpp;
                coppySpecFolder(folder_testCpp, temp_srcTests, temp_destTests); // copy folder test_ModuleName
                coppySpecFile(fileName_testmk, temp_srcTestsModName, temp_destTestsModName); // copy test.mk 
                coppySpecFile(fileName_testmk, temp_srcTestsModName, directoryPath_scr); // copy test.mk to scripts folder                
                coppySpecFile(fileName_ipg, temp_srcTestsModName, temp_destTestsModName); // copy ipg.cop
                coppySpecFile(fileName_ipg, temp_srcTestsModName, directoryPath_scr); // copy ipg.cop to scripts folder
                coppySpecFile(file_testCpp, temp_srcTestsModName, temp_destTestsModName); // copy test_ModuleName.cpp
                coppySpecFile(file_testCpp, temp_srcTestsModName, directoryPath_scr);  // copy test_ModuleName.cpp to scripts folder
                //zip folder
                string startPath = t_srcTestFolder;
                string zipPath = t_srcTestFolder + ".zip";
                System.IO.Compression.ZipFile.CreateFromDirectory(startPath, zipPath);// create zip folder module test   
                System.IO.Directory.Delete(t_srcTestFolder, true); // delete folder Module test
              
                //show
                mesage = "FINISH";
                MessageBox.Show(mesage, "COPY");
             }
            else 
            {
                mesage = "Directory not found";
                MessageBox.Show(mesage, "ERROR");
            }

        }
        private void coppySpecFile(string t_filename, string t_srcFile, string t_destFile)
        {
            if (System.IO.Directory.Exists(t_srcFile) && System.IO.Directory.Exists(t_destFile))
            {
                
                string fileNameTest;
                string[] files = System.IO.Directory.GetFiles(t_srcFile, t_filename);
                foreach (string file_index in files)
                {
                    bool b_check = false;
                    string temp_dest;
                    fileNameTest = System.IO.Path.GetFileName(file_index);
                    if (fileNameTest == except_file1 || fileNameTest == except_file2 || fileNameTest == except_file3) 
                        {
                            b_check = true;
                    
                         }
                    if (b_check == false)
                    {
                        temp_dest = System.IO.Path.Combine(t_destFile, fileNameTest);
                        System.IO.File.Copy(file_index, temp_dest, true);
                        //reset temp_dest
                        temp_dest = "";
                    }
                }
            }
            else
            {
                mesage = "Directory not found";
                MessageBox.Show(mesage, "ERROR");

            }
        }
        private void coppyAllFile(string t_source, string t_destFile)
        {
            if (System.IO.Directory.Exists(t_source) && System.IO.Directory.Exists(t_destFile))
                {
                
                string fileNameTest;
                string[] files = System.IO.Directory.GetFiles(t_source);
                foreach (string file_index in files)
                {
                    bool b_check = false;
                    string temp_dest;
                    fileNameTest = System.IO.Path.GetFileName(file_index);
                      if (fileNameTest == except_file1 || fileNameTest == except_file2 || fileNameTest == except_file3) 
                        {
                            b_check = true;
                    
                         }
                      if (b_check == false)
                      {
                          temp_dest = System.IO.Path.Combine(t_destFile, fileNameTest);
                          System.IO.File.Copy(file_index, temp_dest, true);
                          //reset temp_dest
                          temp_dest = "";
                      }
                }
                }
            else
            {
                mesage = "Directory not found";
                MessageBox.Show(mesage, "ERROR");

            }
        }
            private void coppyAllFolder(string t_source, string t_dest) //not files
            {
           if (System.IO.Directory.Exists(t_source) && System.IO.Directory.Exists(t_dest))
                {
                string[] subdirectoryEntries = System.IO.Directory.GetDirectories(t_source);
                foreach (string subdirectory in subdirectoryEntries)
                {
                   string temp_dest;
                   string folderName = System.IO.Path.GetFileName(subdirectory);
                   temp_dest = System.IO.Path.Combine(t_dest, folderName);
                   CreateFolder(temp_dest);
                }

                }
           else
           {
               mesage = "Directory not found";
               MessageBox.Show(mesage, "ERROR");

           }
            }
            private void coppySpecFolder(string t_specFolderName, string t_source, string t_dest) //not files
            {
                if (System.IO.Directory.Exists(t_source) && System.IO.Directory.Exists(t_dest))
                {
                    string[] subdirectoryEntries = System.IO.Directory.GetDirectories(t_source, t_specFolderName);
                    foreach (string t_directory in subdirectoryEntries)
                    {
                        string temp_dest;
                        string folderName = System.IO.Path.GetFileName(t_directory);
                        temp_dest = System.IO.Path.Combine(t_dest, folderName);
                        CreateFolder(temp_dest);
                    }

                }
                else 
                {
                    mesage = "Directory not found";
                    MessageBox.Show(mesage, "ERROR");
                
                }
             
            }
        private void CreateFolder(string t_source) //not files
        {
            if (!System.IO.Directory.Exists(t_source))

                System.IO.Directory.CreateDirectory(t_source);
            else
            {
                string mesage = "Folder is already exist";
                MessageBox.Show(mesage, "ERROR");
                
            
            }

        }

